{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "target": "ES6",
    "strict": true,
    "allowJs": true,
    "jsx": "react-jsx"
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
